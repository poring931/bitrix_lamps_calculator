; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/lightup/js/goals.js?1611819019823";s:6:"source";s:36:"/local/templates/lightup/js/goals.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
document.addEventListener('DOMContentLoaded', ()=>{
	$form_call_back = document.querySelector('#FORM10');
	if($form_call_back){
		$form_call_back.addEventListener('submit', ()=>{
			yaCounter42766159.reachGoal('send_form_header');
			gtag('event', 'submit_form_header', { 'event_category': 'send_form_header', 'event_action': 'submit'});
			console.log('Event: send_form_header');
		});
	}

	$form_calc = document.querySelector('#FORM7');
	if($form_calc){
		$form_calc.addEventListener('submit', ()=>{
			gtag('event', 'submit_form_header', { 'event_category': 'sent_form_raschet',  'event_action': 'submit'});
		});
	}
	
	$form_price = document.querySelector('#FORM1');
	if($form_price){
		$form_price.addEventListener('submit', ()=>{
			yaCounter42766159.reachGoal('prais_request');
		});
	}
	
});
