$(document).ready(function(){

	// $('.my_catalog_left_menu_item').click(function() {
	// 	$(this).find('.my_left_second_item ').toggle();
	// });
	$('.wrapGeoIpReaspekt').click(function() {
		$('.menu_list_hidden').css('display', 'none');
		$('.overlay').css('display', 'none');
	});
	$('.linkReaspekt').click(function() {
		$('.menu_list_hidden').css('display', 'none');
		$('.overlay').css('display', 'none');
	});

  function OpenTab(e){
    id = $(this).attr('data-tab');

    $('.product-tabs .tab-list li').removeClass('active');
    $('.product-tabs-content div').removeClass('active');

    $('.product-tabs .tab-list').find('span[data-tab="'+id+'"]').parent().addClass('active');
    $('.product-tabs-content div#'+id).addClass('active');
  }

  $('body span[data-tab]').click(OpenTab);

  $('.count-btn.minus').click(function () {
			var $input = $(this).parent().find('input');
			var count = parseInt($input.val()) - 1;
			count = count < 1 ? 1 : count;
			$input.val(count);
			$input.change();
			return false;
		});
	$('.count-btn.plus').click(function () {
		var $input = $(this).parent().find('input');
		$input.val(parseInt($input.val()) + 1);
		$input.change();
		return false;
	});

  $('.buy-button').on('click', 'div', function(){
    var value = $('.quantity input').val();
        prod_name = $('.product-title').text();
        form_id = $(this).attr('data-target');

      $(form_id+' form textarea').text(' '+ prod_name + '   ' + value + ' .');
  });
	$('.my_slick_1').slick({
		slidesToShow: 3,
		slidesToScroll: 1,
		prevArrow: '.my_prev_1',
		nextArrow: '.my_next_1',
		responsive: [
		{
			breakpoint: 1010,
			settings: {
				slidesToShow: 2,
				slidesToScroll: 1,
			}
		},
		{
			breakpoint: 769,
			settings: {
				slidesToShow: 1,
				slidesToScroll: 1,
				prevArrow: '.my_prev_2',
				nextArrow: '.my_next_2',
			}
		}]
	});
	$('.my_slick_3').slick({
		slidesToShow: 3,
		slidesToScroll: 1,
		prevArrow: '.my_prev_4',
		nextArrow: '.my_next_4',
		responsive: [
		{
			breakpoint: 1010,
			settings: {
				slidesToShow: 2,
				slidesToScroll: 1,
			}
		},
		{
			breakpoint: 769,
			settings: {
				slidesToShow: 1,
				slidesToScroll: 1,
				prevArrow: '.my_prev_3',
				nextArrow: '.my_next_3',
			}
		}]
	});
});

/* End */
